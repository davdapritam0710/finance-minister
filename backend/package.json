    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
